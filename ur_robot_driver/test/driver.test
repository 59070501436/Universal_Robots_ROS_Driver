<launch>
  <node name="docker_container" pkg="ur_robot_driver" type="un_docker_ursim.sh" respawn="false" output="screen">
  </node>
  <include file="$(find ur_robot_driver)/launch/ur5e_bringup.launch">
    <arg name="robot_ip" value="172.17.0.2"/>
    <arg name="headless_mode" value="true"/>
  </include>

  <test test-name="switch_on_test" pkg="ur_robot_driver" type="switch_on_test.py" name="switch_on_test1" time-limit="1000.0"/>
  <test test-name="io_test" pkg="ur_robot_driver" type="io_test.py" name="io_test1" time-limit="1000.0"/>
  <test test-name="trajectory_test" pkg="ur_robot_driver" type="trajectory_test.py" name="traj_test1" time-limit="1000.0"/>
</launch>
